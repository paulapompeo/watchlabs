ay:flex;\n    align-items: center;\n    background-color: $bege;\n    height: 80px;\n    width: 280px;\n    h3 {\n      color: $white;\n      font-size: 32px;\n      font-family: 'Montserrat', sans-serif;\n      font-weight: bold;\n      margin-left: 18px;\n      line-height: 40px;\n      letter-spacing: 3px;\n    }\n  }\n\n  .menu-list{\n    ul {\n      display: flex;\n      li {\n        display: flex;\n        align-content: space-between;\n        justify-content: space-between;\n\n        a {\n\t\t\t\t\ttext-decoration: none;\n\t\t\t\t\tfont-size: 16px;\n\t\t\t\t\tcolor: $black;\n          font-weight: 700;\n          margin-right: 80px;\n          font-size: 14px;\n          font-family: 'Montserrat', sans-serif;\n          font-weight: normal;\n          margin-left: 18px;\n          white-space: nowrap;\n\n\t\t\t\t\t&:hover {\n\t\t\t\t\t\ttext-decoration: underline;\n          }\n        }\n      }\n    }\n  }\n\n  .search {\n    display: flex;\n    align-items: center;\n    input {\n      border: none;\n      border-bottom: 1px solid;\n      width: 88px;\n    }\n    button {\n\t\t\tfont-size: 16px;\n\t\t\tfont-weight: 700;\n\t\t\tpadding: 11px 12px;\n\t\t\tborder: none;\n      background-color: $white;\n      margin-right: 88px;\n\t\t}\n  }\n}\n\n.banner-container{\n  display: flex;\n  flex: 1;\n  // align-items: center;\n  justify-content: center;\n  height: 709px;\n  max-width: 1550px;\n\n  background: \n  linear-gradient(\n          rgba(0, 0, 0, 0.4), \n          rgba(0, 0, 0, 0.4)\n  ),\n  url(assets/banner.png) no-repeat center;\n  background-size: cover;\n\n  .banner-content {\n    margin-top: 150px;\n    text-align: center;\n\n    span {\n      font-family: 'Montserrat', sans-serif;\n      font-weight: bold;\n      color: $white;\n    }\n\n    h4 {\n      //trocar a fonte aqui - tipo que nao temos\n      //ver como corta a linha em 2\n      font-family: 'Montserrat', sans-serif;\n      font-weight: bold;\n      color: $white;\n    }\n\n    p {\n      //trocar a fonte aqui - tipo que nao temos\n      font-family: 'Montserrat', sans-serif;\n      font-weight: normal;\n      color: $white;\n      margin-top: 24px;\n    }\n\n  }\n}"},"lineCount":null}},"error":null,"hash":"144a59d424cbb617cff821c3c002fc92","cacheData":{"env":{}}}